# Configurações da API
NEXT_PUBLIC_API_URL=https://api.topautomacoes.com

# Configurações de autenticação (se necessário)
API_SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Configurações do banco de dados (se você tiver um backend próprio)
DATABASE_URL=postgresql://user:password@localhost:5432/dashboard_db

# Configurações para embedding
ALLOWED_ORIGINS=https://exemplo.com,https://outro-site.com

# Configurações de notificações
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=seu-email@gmail.com
SMTP_PASS=sua-senha-app

# Configurações do Webhook
WEBHOOK_SECRET=webhook_secret_key

# Configurações de desenvolvimento
NODE_ENV=development
PORT=3000 